<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
    "http://www.ibatis.com/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings
		lazyLoadingEnabled="true" 
		cacheModelsEnabled="true"
		enhancementEnabled="false" 
		errorTracingEnabled="true"
		maxRequests="128"
		maxSessions="64"
		maxTransactions="8"
		useStatementNamespaces="true"
	/>
	
	<sqlMap resource="com/tb/dao/impl/map/user_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/entry_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/file_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/mail_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/message_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/votetheme_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/votequiz_SqlMap.xml"/>
	<sqlMap resource="com/tb/dao/impl/map/voteoption_SqlMap.xml"/>
	
	
</sqlMapConfig>